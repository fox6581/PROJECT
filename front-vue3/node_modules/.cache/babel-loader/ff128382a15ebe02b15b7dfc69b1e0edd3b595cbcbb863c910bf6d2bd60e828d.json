{"ast":null,"code":"import { mapState, mapGetters, mapActions } from 'vuex';\nimport { API_BASE_URL } from '@/config';\nimport ShowButton from '../Buttons/ShowButton.vue';\nexport default {\n  data() {\n    return {\n      API_BASE_URL\n    };\n  },\n  components: {\n    ShowButton\n  },\n  computed: {\n    ...mapState(['cart']),\n    ...mapGetters(['totalPrice'])\n  },\n  methods: {\n    ...mapActions(['removeFromCart', 'increaseQuantity', 'decreaseQuantity'])\n  }\n};","map":{"version":3,"names":["mapState","mapGetters","mapActions","API_BASE_URL","ShowButton","data","components","computed","methods"],"sources":["C:\\shopvue\\front-vue3\\src\\components\\Basket\\BasketProduct.vue"],"sourcesContent":["<template>\r\n    <div class=\"all\">\r\n\r\n        <div class=\"cart\">\r\n            <h2>Корзина</h2>\r\n            <div v-if=\"cart.length > 0\">\r\n                <div v-for=\"(item, index) in cart\" :key=\"index\" class=\"cart-item\">\r\n                    <div>\r\n                        <img :src=\"`${API_BASE_URL}/${item.images[0]}`\" alt=\"Изображение товара\" class=\"item-image\" />\r\n                    </div>\r\n\r\n                    <div class=\"item-details\">\r\n                        <div>\r\n                            <p><strong></strong> {{ item.name || 'Неизвестный товар' }}</p>\r\n\r\n                        </div>\r\n\r\n\r\n                        <div class=\"quantity-controls\">\r\n                            <img src=\"@/assets/No Edit.png\" @click=\"decreaseQuantity(item.id)\" width=\"24\" height=\"24\"\r\n                                alt=\"\">\r\n                            <!-- <button @click=\"decreaseQuantity(item.id)\">-</button> -->\r\n                            <span>{{ item.quantity }}</span>\r\n                            <button>\r\n                                <img src=\"@/assets/Edit.png\" @click=\"increaseQuantity(item.id)\" width=\"24\" height=\"24\"\r\n                                    alt=\"\">\r\n          