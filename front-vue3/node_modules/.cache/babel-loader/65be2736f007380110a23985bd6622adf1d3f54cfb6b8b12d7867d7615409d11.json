n    <div class=\"container-wrapper\">\r\n      <!-- Заголовок категории -->\r\n      <div class=\"strelki\">\r\n        <h2>Browse By Category</h2>\r\n        <div style=\"display: flex;\">\r\n          <!-- Стрелка влево -->\r\n          <img\r\n            class=\"arrow-button left\"\r\n           :src=\"require('@/assets/Arrow.png')\"\r\n            alt=\"left arrow\"\r\n            width=\"32\"\r\n            height=\"32\"\r\n            @click=\"scrollLeft\"\r\n          />\r\n          <!-- Стрелка вправо -->\r\n          <img\r\n            class=\"arrow-button right\"\r\n            :src=\"arrowSrc\"\r\n            alt=\"right arrow\"\r\n            width=\"32\"\r\n            height=\"32\"\r\n            @click=\"scrollRight\"\r\n          />\r\n        </div>\r\n      </div>\r\n  \r\n      <!-- Контейнер с блоками -->\r\n      <div class=\"container\">\r\n        <div\r\n          v-for=\"(block, index) in blocks\"\r\n          :key=\"index\"\r\n          class=\"block\"\r\n          @click=\"handleClick(block.link)\"\r\n        >\r\n          <img :src=\"require(`@/assets/${block.image}`)\" :alt=\"block.name\" width=\"48\" height=\"48\" />\r\n          <p>{{ block.name }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script setup>\r\n  import { ref } from 'vue';\r\n  \r\n  // Массив с блоками\r\n  const blocks = ref([\r\n    { name: 'Phones', image: 'Phones.png', link: '/phones' },\r\n    { name: 'Smart Watches', image: 'Smart Watches.png', link: '/phones' },\r\n    { name: 'Cameras', image: 'Phones.png', link: '/phones' },\r\n    { name: 'Headphones', image: 'Headphones.png', link: '/phones' },\r\n    { name: 'Computers', image: 'Computers.png', link: '/phones' },\r\n    { name: 'Gaming', image: 'Gaming.png', link: '/phones' },\r\n  ]);\r\n  \r\n  const arrowSrc = '@/assets/Arrow.png'; // Путь к изображению стрелки\r\n  \r\n  // Функция для прокрутки влево\r\n  const scrollLeft = () => {\r\n    const firstBlock = blocks.value.shift(); // Убираем первый элемент массива\r\n    blocks.value.push(firstBlock); // Добавляем его в конец массива\r\n  };\r\n  \r\n  // Функция для прокрутки вправо\r\n  const scrollRight = () => {\r\n    const lastBlock = blocks.value.pop(); // Убираем последний элемент массива\r\n    blocks.value.unshift(lastBlock); // Добавляем его в начало массива\r\n  };\r\n  \r\n  const handleClick = (link) => {\r\n    console.log(`Redirecting to: ${link}`);\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .strelki {\r\n    display: flex;\r\n    width: 1120px;\r\n    justify-content: space-between;\r\n  }\r\n  \r\n  .container-wrapper {\r\n    display: flex;\r\n    align-items: center;\r\n    flex-direction: column;\r\n  }\r\n  \r\n  .arrow-button {\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .arrow-button.left {\r\n    transform: rotate(180deg); /* Поворот для левой стрелки */\r\n  }\r\n  \r\n  .container {\r\n    display: flex;\r\n    column-gap: 20px;\r\n    width: 1120px;\r\n    margin: auto;\r\n    overflow-x: hidden; /* Скрыть не видимые блоки */\r\n  }\r\n  \r\n  .block {\r\n    font-family: ABeeZee;\r\n    font-size: 16px;\r\n    font-style: italic;\r\n    font-weight: 400;\r\n    line-height: 24px;\r\n    text-align: center;\r\n    color: #000;\r\n    width: 160px;\r\n    height: 128px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    background: #EDEDED;\r\n    border-radius: 15px;\r\n    justify-content: center;\r\n  }\r\n  \r\n  .block p {\r\n    font-size: 16px;\r\n    font-weight: 500;\r\n    margin-top: 8px;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA2CE,SAASA,GAAG,QAAQ,KAAK;;AAEzB;AAUA,MAAMC,QAAQ,GAAG,oBAAoB,CAAC,CAAC;;AAEvC;;;;;;;;IAXA,MAAMC,MAAM,GAAGF,GAAG,CAAC,CACjB;MAAEG,IAAI,EAAE,QAAQ;MAAEC,KAAK,EAAE,YAAY;MAAEC,IAAI,EAAE;IAAU,CAAC,EACxD;MAAEF,IAAI,EAAE,eAAe;MAAEC,KAAK,EAAE,mBAAmB;MAAEC,IAAI,EAAE;IAAU,CAAC,EACtE;MAAEF,IAAI,EAAE,SAAS;MAAEC,KAAK,EAAE,YAAY;MAAEC,IAAI,EAAE;IAAU,CAAC,EACzD;MAAEF,IAAI,EAAE,YAAY;M