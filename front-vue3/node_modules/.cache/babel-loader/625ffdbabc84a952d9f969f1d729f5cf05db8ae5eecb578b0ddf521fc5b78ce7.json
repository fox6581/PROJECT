{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.for-each.js\";\nimport { API_BASE_URL } from '@/config';\nconsole.log('API_BASE_URL:', API_BASE_URL); // Добавьте это для отладки\nexport default {\n  name: 'HeaderSearch',\n  props: {\n    placeholder: {\n      type: String,\n      default: 'Search...'\n    },\n    icon: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      query: '',\n      results: []\n    };\n  },\n  watch: {\n    query: 'searchProducts'\n  },\n  methods: {\n    async searchProducts() {\n      if (this.query.length < 2) {\n        this.results = [];\n        return;\n      }\n      try {\n        console.log(`Fetching from: ${API_BASE_URL}/api/products?name=${this.query}`);\n        const response = await fetch(`${API_BASE_URL}/api/products?name=${this.query}`);\n        if (!response.ok) {\n          throw new Error('Ошибка при запросе данных');\n        }\n        const data = await response.json();\n        this.results = data;\n\n        // Проверка URL для первого изображения каждого продукта\n        this.results.forEach(product => {\n          if (product.images && product.images.length > 0) {\n            const imageUrl = `${API_BASE_URL}/${product.images[0]}`;\n            console.log('Final Image URL:', `${imageUrl}/${product.images[0]}`);\n          } else {\n            console.log('Нет изображений для продукта:', product);\n          }\n        });\n      } catch (error) {\n        console.error(\"Ошибка поиска:\", error);\n      }\n    },\n    selectProduct(product) {\n      this.$emit('selectProduct', product);\n      this.query = '';\n      this.results = [];\n    }\n  }\n};","map":{"version":3,"names":["API_BASE_URL","console","log","name","props","placeholder","type","String","default","icon","required","data","query","results","watch","methods","searchProducts","length","response","fetch","ok","Error","json","forEach","product","images","imageUrl","error","selectProduct","$emit"],"sources":["C:\\shopvue\\front-vue3\\src\\components\\Search\\HeaderSearch.vue"],"sourcesContent":["<template>\r\n  <div class=\"header-search\">\r\n    <img :src=\"icon\" alt=\"Search Icon\" class=\"search-icon\" />\r\n    <input type=\"text\" :placeholder=\"placeholder\" v-model=\"query\" />\r\n\r\n    <!-- Выпадающий список результатов поиска -->\r\n    <div v-if=\"results.length\" class=\"search-results\">\r\n      <div v-for=\"product in results\" :key=\"product.id\" class=\"search-result-item\" @click=\"selectProduct(product)\">\r\n        <!-- <img :src=\"`http://localhost:1452/${product.images[0]}`\" alt=\"Product Image\" class=\"result-image\" /> -->\r\n        <!-- <img :src=\"`${API_BASE_URL}/${product.images[0]}`\" alt=\"Product Image\" class=\"result-image\" /> -->\r\n        <img v-if=\"product.images && product.images.length > 0\" :src=\"`${API_BASE_URL}/${product.images[0]}`\"\r\n          alt=\"Product Image\" class=\"result-image\" />\r\n\r\n        <div class=\"result-info\">\r\n          <h3>{{ product.name }}</h3>\r\n          <p>Цена: {{ product.discount_price || product.price }} руб</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { API_BASE_URL } from '@/config';\r\n\r\n\r\nconsole.log('API_BASE_URL:', API_BASE_URL); // Добавьте это для отладки\r\nexport default {\r\n  name: 'HeaderSearch',\r\n\r\n  props: {\r\n    placeholder: {\r\n      type: String,\r\n      default: 'Search...',\r\n    },\r\n    icon: {\r\n      type: String,\r\n      required: true,\r\n    },\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      query: '',\r\n      results: [],\r\n    };\r\n  },\r\n\r\n  watch: {\r\n    query: 'searchProducts',\r\n  },\r\n\r\n  methods: {\r\n    async searchProducts() {\r\n      if (this.query.length < 2) {\r\n        this.results = [];\r\n        return;\r\n      }\r\n\r\n      try {\r\n        console.log(`Fetching from: ${API_BASE_URL}/api/products?name=${this.query}`);\r\n        const response = await fetch(`${API_BASE_URL}/api/products?name=${this.query}`);\r\n\r\n        if (!response.ok) {\r\n          throw new Error('Ошибка при запросе данных');\r\n        }\r\n\r\n        const data = await response.json();\r\n        this.results = data;\r\n\r\n        // Проверка URL для первого изображения каждого продукта\r\n        this.results.forEach(product => {\r\n          if (product.images && product.images.length > 0) {\r\n            const imageUrl = `${API_BASE_URL}/${product.images[0]}`;\r\n            console.log('Final Image URL:', `${imageUrl}/${product.images[0]}`);\r\n          } else {\r\n            console.log('Нет изображений для продукта:', product);\r\n          }\r\n        });\r\n      } catch (error) {\r\n        console.error(\"Ошибка поиска:\", error);\r\n      }\r\n    },\r\n\r\n    selectProduct(product) {\r\n      this.$emit('selectProduct', product);\r\n      this.query = '';\r\n      this.results = [];\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.header-search {\r\n  position: relative;\r\n}\r\n\r\n.header-search input {\r\n  padding-left: 40px;\r\n  width: 372px;\r\n  height: 56px;\r\n  padding: 16px;\r\n  gap: 8px;\r\n  border-radius: 8px;\r\n  background: #f5f5f5;\r\n  font-family: ABeeZee, sans-serif;\r\n  font-size: 14px;\r\n  font-style: italic;\r\n  font-weight: 400;\r\n  line-height: 18px;\r\n  text-align: left;\r\n  text-indent: 35px;\r\n}\r\n\r\n.search-icon {\r\n  position: absolute;\r\n  left: 12px;\r\n  top: 50%;\r\n  transform: translateY(-50%);\r\n  width: 24px;\r\n  height: 24px;\r\n}\r\n\r\n\r\n\r\n.search-results {\r\n  position: absolute;\r\n  background-color: white;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  width: 100%;\r\n  max-height: 200px;\r\n  /* Максимальная высота для выпадающего списка */\r\n  overflow-y: auto;\r\n  /* Позволяет прокручивать, если содержимое превышает высоту */\r\n  z-index: 1000;\r\n}\r\n\r\n.search-result-item {\r\n  display: flex;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n}\r\n\r\n.search-result-item:hover {\r\n  background-color: #f0f0f0;\r\n}\r\n\r\n.result-image {\r\n  width: 50px;\r\n  height: 50px;\r\n  margin-right: 10px;\r\n}\r\n\r\n.result-info {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n</style>"],"mappings":";;AAuBA,SAASA,YAAW,QAAS,UAAU;AAGvCC,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEF,YAAY,CAAC,EAAE;AAC5C,eAAe;EACbG,IAAI,EAAE,cAAc;EAEpBC,KAAK,EAAE;IACLC,WAAW,EAAE;MACXC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDC,IAAI,EAAE;MACJH,IAAI,EAAEC,MAAM;MACZG,QAAQ,EAAE;IACZ;EACF,CAAC;EAEDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EAEDC,KAAK,EAAE;IACLF,KAAK,EAAE;EACT,CAAC;EAEDG,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB,IAAI,IAAI,CAACJ,KAAK,CAACK,MAAK,GAAI,CAAC,EAAE;QACzB,IAAI,CAACJ,OAAM,GAAI,EAAE;QACjB;MACF;MAEA,IAAI;QACFZ,OAAO,CAACC,GAAG,CAAC,kBAAkBF,YAAY,sBAAsB,IAAI,CAACY,KAAK,EAAE,CAAC;QAC7E,MAAMM,QAAO,GAAI,MAAMC,KAAK,CAAC,GAAGnB,YAAY,sBAAsB,IAAI,CAACY,KAAK,EAAE,CAAC;QAE/E,IAAI,CAACM,QAAQ,CAACE,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,2BAA2B,CAAC;QAC9C;QAEA,MAAMV,IAAG,GAAI,MAAMO,QAAQ,CAACI,IAAI,CAAC,CAAC;QAClC,IAAI,CAACT,OAAM,GAAIF,IAAI;;QAEnB;QACA,IAAI,CAACE,OAAO,CAACU,OAAO,CAACC,OAAM,IAAK;UAC9B,IAAIA,OAAO,CAACC,MAAK,IAAKD,OAAO,CAACC,MAAM,CAACR,MAAK,GAAI,CAAC,EAAE;YAC/C,MAAMS,QAAO,GAAI,GAAG1B,YAAY,IAAIwB,OAAO,CAACC,MAAM,CAAC,CAAC,CAAC,EAAE;YACvDxB,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAE,GAAGwB,QAAQ,IAAIF,OAAO,CAACC,MAAM,CAAC,CAAC,CAAC,EAAE,CAAC;UACrE,OAAO;YACLxB,OAAO,CAACC,GAAG,CAAC,+BAA+B,EAAEsB,OAAO,CAAC;UACvD;QACF,CAAC,CAAC;MACJ,EAAE,OAAOG,KAAK,EAAE;QACd1B,OAAO,CAAC0B,KAAK,CAAC,gBAAgB,EAAEA,KAAK,CAAC;MACxC;IACF,CAAC;IAEDC,aAAaA,CAACJ,OAAO,EAAE;MACrB,IAAI,CAACK,KAAK,CAAC,eAAe,EAAEL,OAAO,CAAC;MACpC,IAAI,CAACZ,KAAI,GAAI,EAAE;MACf,IAAI,CAACC,OAAM,GAAI,EAAE;IACnB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}